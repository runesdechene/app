{
  "cli": {
    "version": ">= 11.0.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.guildedesvoyageurs.fr/"
      }
    },
    "apk": {
      "autoIncrement": true,
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.guildedesvoyageurs.fr/"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "rewieer@gmail.com",
        "ascAppId": "6733223222",
        "appleTeamId": "D23SL6S6W7"
      }
    }
  }
}
