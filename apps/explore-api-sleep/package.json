  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pnpm orm:up && node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ./vitest.config.js",
    "test:int": "vitest run --config ./vitest.int.config.js",
    "test:watch": "vitest --config ./vitest.config.js",
    "test:all": "pnpm test && pnpm test:int",
    "test:coverage": "vitest run --config ./vitest.config.js --coverage",
    "orm:create-initial": "npx mikro-orm-esm --config mikro-orm.config.ts migration:create --initial",
    "orm:create": "npx mikro-orm-esm --config mikro-orm.config.ts migration:create",
    "orm:up": "npx mikro-orm-esm --config mikro-orm.config.ts migration:up",
    "orm:down": "npx mikro-orm-esm --config mikro-orm.config.ts migration:down",
    "orm": "npx mikro-orm-esm --config mikro-orm.config.ts",
    "cli": "npx tsx src/cli.ts"
  },
