{
  "expo": {
    "newArchEnabled": true,
    "name": "Runes de Chêne EXPLORE",
    "slug": "guilde-des-voyageurs",
    "version": "2.1.1",
    "orientation": "portrait",
    "icon": "./src/ui/assets/images/icon.png",
    "scheme": "gdv",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "splash": {
      "image": "./src/ui/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fr.guildedesvoyageurs.app",
      "config": {
        "googleMapsApiKey": "AIzaSyCYatDWKxDBI0uZWLR7yEThfFs8SrJeHbM"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD2xJi5hUNU1UV0z0inL6XLQESefUTXNek"
        }
      },
      "package": "fr.guildedesvoyageurs.app",
      "adaptiveIcon": {
        "foregroundImage": "./src/ui/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/ui/assets/images/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Cette application accède à votre position pour vous montrer des lieux tout autour de vous.",
          "locationAlwaysPermission": "Cette application accède à votre position pour vous montrer des lieux tout autour de vous.",
          "locationWhenInUsePermission": "Cette application accède à votre position pour vous montrer des lieux tout autour de vous."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Cette application accède à vos photos lorsque vous souhaitez ajouter des lieux."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "147a6cdc-2328-4d3e-8544-862719950ade"
      }
    },
    "owner": "runes-de-chene"
  }
}